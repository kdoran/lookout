{
  "name": "couchlookout",
  "version": "0.0.0-development",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "copy:index": "cp ./index.html dist/index.html && cp ./package.json dist/package.json",
    "build": "webpack && npm run copy:index",
    "deploy": "gh-pages -d dist -r https://$GH_TOKEN@github.com/kdoran/lookout.git -x true",
    "test": "standard",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "license": "MIT",
  "dependencies": {
    "react": "^16.4.1",
    "react-ace": "^6.1.0",
    "react-dom": "^16.4.1",
    "react-onclickout": "^2.0.8",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.10",
    "gh-pages": "^2.0.1",
    "semantic-release": "^15.10.5",
    "standard": "^11.0.1",
    "style-loader": "^0.20.3",
    "travis-deploy-once": "^5.0.9",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/dist/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kdoran/lookout.git"
